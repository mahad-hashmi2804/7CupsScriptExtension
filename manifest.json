{
  "manifest_version": 2,
  "name": "Google Doc Viewer",
  "version": "1.0",
  "description": "Displays a Google Doc in a Bootstrap offcanvas.",
  "content_scripts": [
    {
      "matches": ["https://www.7cups.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "icon_16.png",
    
    "128": "icon_128.png"
  },
  "browser_action": {
    // "default_popup": "popup.html",
    "default_icon": {
      "16": "icon_16.png",
      "128": "icon_128.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}



